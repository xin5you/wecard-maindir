<?xml version="1.0" encoding="UTF-8"?>  
<weblogic-web-app xmlns="http://xmlns.oracle.com/weblogic/weblogic-web-app" xmlns:j2ee="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://xmlns.oracle.com/weblogic/weblogic-web-app http://xmlns.oracle.com/weblogic/weblogic-web-app/1.3/weblogic-web-app.xsd">  
	<context-root>/</context-root>
	<container-descriptor>
		<prefer-web-inf-classes>false</prefer-web-inf-classes>
		<show-archived-real-path-enabled>true</show-archived-real-path-enabled>
		<prefer-application-packages>
			<package-name>org.slf4j</package-name>
		</prefer-application-packages>
	</container-descriptor>
</weblogic-web-app>